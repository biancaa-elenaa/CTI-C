class Figura{
    private float arie;

    public Figura(float arie){
	this.arie=arie;
    }

    private boolean equals(Object o)
    {
	if(o instaceof Figura){
	    return ((Figura).o).getArie() == this.arie;
	}
	return false;
    }

    public float getArie(){
	return this.arie;
    }
}
